{
  "name": "@kreattix/react",
  "version": "0.0.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "cd example && npm uninstall @kreattix/react && npm install @kreattix/react@file:.. && npm run dev",
